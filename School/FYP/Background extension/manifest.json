{
    "manifest_version": 3,
    "name": "GoogleBackground",
    "description": "Add a custom background to Google.",
    "version": "1.0",
    "icons": 
    {
        "16": "/images/icons-16.png",
        "48": "/images/icons-48.png",
        "128": "/images/icons-100.png"
    },
    "action": 
    {
        "default-icon": 
        {
            "16": "/images/icons-16.png",
            "48": "/images/icons-48.png",
            "128": "/images/icons-100.png"
        },
        "default_title": "Google Background",
        "default_popup": "popup.html"
    },
    "content_scripts":[
    {
        "matches": ["https://www.google.com/*"],
        "css": ["main.css"],
        "js": ["JavaScript.js"]
    }
]
}